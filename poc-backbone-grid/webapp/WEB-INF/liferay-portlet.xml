		<header-portlet-css>/css/main.css</header-portlet-css>
		<css-class-wrapper>LoginPortlet</css-class-wrapper>
	</portlet>
	
	<portlet>
        <portlet-name>HomePagePortlet</portlet-name>
        <icon>/icon.png</icon>
        <instanceable>true</instanceable>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <css-class-wrapper>LoginPortlet</css-class-wrapper>
    </portlet>

